.zh-slide{background-color:rgba(0,0,0,0.3);height:100%;width:100%;position:absolute;z-index:1000;top:0}@keyframes slide{0%{transform:translateX(-100%)}100%{transform:translateX(0)}}.zh-slide .slide-left{overflow-y:scroll;height:100%;width:70%;background-color:#fff;transform:translateX(0);animation:slide .5s linear}.zh-slide .slide-left .header{height:150rpx;width:100%;background-color:#008bed}.zh-slide .slide-left .slide-list{padding-left:18rpx}.zh-slide .slide-left .slide-list .list-item{height:96rpx;line-height:96rpx}.zh-slide .slide-left .slide-list .list-item .add-fa{height:36rpx;width:36rpx;background-image:url('../access/images/add-fa.svg') no-repeat;background-size:100%}